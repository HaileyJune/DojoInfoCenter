
<strong>Latest Activities:</strong>
<ul>
@{
    if (@ViewBag.LatestMessages.Count != 0)
    {
        foreach(var m in @ViewBag.LatestMessages)
        {
            <li>
                <strong>@m.Location.LocationName:</strong> @m.MessageTxt 
                <br>
                by @m.Creator.UserName @m.CreatedAt.ToString("MM/dd h:mm tt")
            </li>
        }
    }
}
</ul>